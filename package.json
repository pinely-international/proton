{
  "type": "module",
  "name": "@denshya/proton",
  "version": "0.1.175",
  "private": false,
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/denshya"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build",
    "globals.d.ts",
    "jsx-runtime.js",
    "jsx-runtime.d.ts"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && bun build:types",
    "build:types": "tsup src/index.ts src/globals.ts --outDir build --format esm --dts-only --tsconfig tsconfig.build.json",
    "preview": "vite preview",
    "release:patch": "npm version patch && bun run build && export GITHUB_TOKEN=${GITHUB_TOKEN} && npm publish",
    "stats": "bun stats:treemap && bun stats:sunburst",
    "stats:treemap": "vite-bundle-visualizer --sourcemap -o ./stats/treemap.html",
    "stats:sunburst": "vite-bundle-visualizer -o ./stats/sunburst.html -t sunburst",
    "stats:network": "vite-bundle-visualizer -o ./stats/network.html -t network"
  },
  "peerDependencies": {
    "type-fest": "^4.31.0",
    "@denshya/flow": "^0.1.3"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "eslint": "9.17.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-externalize-deps": "^0.8.0"
  }
}
